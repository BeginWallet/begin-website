generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model pools {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model registration {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  nonce       String
  userAddress String
  walletAddress String
}
